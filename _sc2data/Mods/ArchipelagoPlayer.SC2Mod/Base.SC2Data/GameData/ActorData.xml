<?xml version="1.0" encoding="utf-8"?>
<Catalog>
    <CActorUnit id="AP_Medic" parent="GenericUnitStandard" unitName="AP_Medic">
        <Aliases value="_UnitSmall"/>
        <Macros value="UnloadDropAnim"/>
        <On Terms="Abil.heal.SourceChannelStart" Send="$Spell PlayForever"/>
        <On Terms="Abil.heal.SourceChannelStop" Send="AnimClear Spell"/>
        <On Terms="Behavior.KillSquad.On" Send="ModelSwap MedicKillSquad"/>
        <On Terms="UnitDeathCustomize; ValidateUnit IsKillSquad" Send="DeathCustomize Variation2"/>
        <DeathArray index="Normal" ModelLink="AP_MedicDeath" SoundLink="AP_Medic_Explode"/>
        <DeathArray index="Blast" ModelLink="AP_MedicDeath"/>
        <DeathCustoms ModelLink="AP_MedicKillSquadDeath" SoundLink="AP_Medic_Death" Name="Variation2"/>
        <PortraitModel value="AP_MedicPortrait"/>
        <WalkAnimMoveSpeed value="2.25"/>
        <BarOffset value="20"/>
        <BarWidth value="36"/>
        <GroupIcon>
            <Image value="Assets\Textures\Wireframe-Terran-medic.dds"/>
        </GroupIcon>
        <HeroIcon value="Assets\Textures\BTN-Unit-Terran-Medic.dds"/>
        <LifeArmorIcon value="Assets\Textures\btn-upgrade-terran-infantryarmorlevel0.dds"/>
        <UnitIcon value="Assets\Textures\BTN-Unit-Terran-Medic.dds"/>
        <Wireframe>
            <Image value="Assets\Textures\Wireframe-Terran-medic.dds"/>
        </Wireframe>
    </CActorUnit>
    <CActorBeamSimple id="AP_MedicHealBeam" parent="BeamSimpleAnimationStyleContinuous">
        <Inherits index="Visibility" value="1"/>
        <On Terms="Effect.AP_heal.TargetStart" Send="Create"/>
        <On Terms="Effect.AP_heal.TargetStop" Send="Destroy"/>
        <HostForProps Subject="_Unit" Scope="Caster" Actor="Find"/>
        <HostLaunch Subject="_Unit" Scope="Caster" Actor="Find"/>
        <HostLaunchSiteOps Ops="SOpAttachWeapon"/>
        <HostImpact Subject="_Unit" Scope="Target" Actor="Find"/>
        <HostImpactSiteOps Ops="SOpAttachCenter"/>
    </CActorBeamSimple>
    <CActorModel id="AP_MedicHealModel" parent="ModelAnimationStyleContinuous">
        <On Terms="ActorCreation" Send="$Birth PlayForever"/>
        <On Terms="Effect.AP_heal.TargetStart; At Target" Send="Create"/>
        <On Terms="Effect.AP_heal.TargetStop; At Target" Send="Destroy"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachOverhead"/>
    </CActorModel>
    <CActorSound id="AP_Medic_Death" parent="SoundOneShot">
        <On Terms="UnitDeath.AP_Medic; PassChance 25.000000" Send="Create"/>
    </CActorSound>
    <CActorUnit id="AP_Firebat" parent="GenericUnitStandard" unitName="AP_Firebat">
        <Aliases value="_UnitMedium"/>
        <Macros value="UnloadDropAnim"/>
        <On Terms="Abil.attack.ReadyStop" Target="AP_FirebatAttackModel" Send="SetOpacity 0.000000 0.250000"/>
        <DeathArray index="Normal" ModelLink="AP_FirebatDeath" SoundLink="AP_Firebat_Explode"/>
        <PortraitModel value="AP_FirebatPortrait"/>
        <WalkAnimMoveSpeed value="1.875"/>
        <BarOffset value="77"/>
        <BarWidth value="45"/>
        <GroupIcon>
            <Image value="Assets\Textures\Wireframe-Terran-firebat.dds"/>
        </GroupIcon>
        <HeroIcon value="Assets\Textures\btn-unit-terran-firebat.dds"/>
        <LifeArmorIcon value="Assets\Textures\btn-upgrade-terran-infantryarmorlevel0.dds"/>
        <UnitIcon value="Assets\Textures\btn-unit-terran-firebat.dds"/>
        <Wireframe>
            <Image value="Assets\Textures\Wireframe-Terran-firebat.dds"/>
        </Wireframe>
    </CActorUnit>
    <CActorAction id="AP_FirebatAttack" parent="GenericAttackBase" effectAttack="Firebat">
        <On Terms="Effect.AP_FirebatSet.Start; At Caster" Send="Create"/>
        <On Terms="Effect.FirebatUFull.Start; At Caster; FromEffectTreeDescendant" Send="ActionDamage"/>
        <LaunchModel value="AP_FirebatAttackModel"/>
        <ContainerAssets Model="AP_FirebatBunkerAttackLaunch" Sound="AP_FirebatBunkerAttackLaunch"/>
        <ImpactMap index="None" Sound="AP_Firebat_AttackImpact"/>
    </CActorAction>
    <CActorModel id="AP_FirebatAttackModel" parent="ModelAnimationStyleOneShotNoOrphan">
        <Flags index="SuppressCreationErrors" value="1"/>
        <On Terms="ActorCreation; ValidateUnit BearclawNozzlesPurchased" Send="SetScale 2.250000,1.800000,1.000000"/>
    </CActorModel>
    <CActorFoliageFXSpawner id="AP_FirebatFoliageFXSpawner" parent="FoliageFXSpawnerFire">
        <On Terms="Effect.AP_FirebatE.Start" Send="Create"/>
        <Radius value="0.600000"/>
    </CActorFoliageFXSpawner>
    <CActorRegionCircle id="AP_FirebatTreeKiller">
        <On Terms="Effect.AP_FirebatE.Start; At Effect" Send="Create"/>
        <On Terms="Effect.AP_DevilDogSearchArea.Start; At Effect" Send="Create"/>
        <On Terms="ActorCreation" Send="QueryRegion ::Self TreeBurnQuery AgainstCenter"/>
        <On Terms="Effect.AP_FirebatE.Start; At Effect" Send="Destroy"/>
        <On Terms="Effect.AP_DevilDogSearchArea.Start; At Effect" Send="Destroy"/>
        <Radius value="0.400000"/>
    </CActorRegionCircle>
    <CActorModel id="AP_ImpactModelFlames" parent="ModelAnimationStyleOneShot">
        <Terms value="Cap 1"/>
        <On Terms="Effect.AP_FirebatUFull.Start; At Target" Send="Create"/>
        <On Terms="Effect.VolatileDeathDamage.Start; At Target" Send="Create"/>
        <On Terms="ActorOrphan" Send="Destroy"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachRandomDamage"/>
    </CActorModel>
    <CActorModel id="AP_ViralPlasmaBurnModel">
        <On Terms="Behavior.AP_ViralPlasma.On" Send="Create"/>
        <On Terms="ActorCreation" Send="$Stand 0 -1.000000 -1.000000 6.000000 AsDuration"/>
        <On Terms="Behavior.AP_ViralPlasma.Off" Send="Destroy"/>
        <Host Subject="_Selectable"/>
        <HostSiteOps Ops="SOpAttachRandomDamage"/>
        <Model value="AP_ImpactModelFlames"/>
    </CActorModel>
    <CActorUnit id="AP_Marine" parent="GenericUnitBase" unitName="AP_Marine">
        <Aliases value="_UnitSmall"/>
        <Macros value="UnloadDropAnim"/>
        <Macros value="PhysicsDeathsSwarmGround"/>
        <Macros value="TerrainSquibAnimations"/>
        <On Terms="ActorCreation; ValidatePlayer NoMarineShield" Send="AnimPlay Cover Cover PlayForever 0.000000"/>
        <On Terms="Upgrade.AP_ShieldWall.Add" Send="AnimClear Cover"/>
        <On Terms="Upgrade.AP_ShieldWall.Remove" Send="AnimPlay Cover Cover PlayForever 0.000000"/>
        <On Terms="UnitDeath.AP_Marine; AnimPlaying Cover" Target="_DeathModel" Send="AnimPlay Cover Cover PlayForever 0.000000"/>
        <On Terms="Abil.attack.ReadyStart" Send="AnimGroupApply Ready"/>
        <On Terms="WeaponStart.*.AttackStart; WeaponTargetElevation GE 20.000000" Send="AnimBracketStart Attack Attack,Superior"/>
        <On Terms="WeaponStart.*.AttackStart; WeaponTargetElevation LE -20.000000" Send="AnimBracketStart Attack Attack,Inferior"/>
        <On Terms="WeaponStart.*.AttackStart" Send="AnimBracketStart Attack Attack"/>
        <On Terms="WeaponStop.*.AttackStop" Send="AnimBracketStop Attack"/>
        <On Terms="Abil.attack.ReadyStop" Send="AnimGroupRemove Ready"/>
        <On Terms="ModelEvent; ModelEventName SquibStart" Send="Create MarineCheerSquib"/>
        <On Terms="ModelEvent; ModelEventName SquibStop" Target="AP_MarineCheerSquib" Send="Destroy"/>
        <On Terms="ModelEvent; ModelEventName SquibStop" Target="MarineCheerSquibSound" Send="Destroy"/>
        <On Terms="AnimDone; AnimName Cheer" Target="AP_MarineCheerSquib" Send="Destroy"/>
        <On Terms="AnimDone; AnimName Cheer" Target="MarineCheerSquibSound" Send="Destroy"/>
        <On Terms="UnitHeightUpdate.*.Land" Send="Create Marine_Land"/>
        <On Terms="Upgrade.AP_CombatShield.Add" Send="AnimClear Cover"/>
        <On Terms="Upgrade.AP_CombatShield.Remove" Send="AnimPlay Cover Cover PlayForever"/>
        <On Terms="Upgrade.AP_CombatShield.Add" Send="AnimClear Cover"/>
        <On Terms="Upgrade.AP_CombatShield.Remove" Send="AnimPlay Cover Cover PlayForever"/>
        <ModelFlags index="OutlineEmitter" value="1"/>
        <DeathArray index="Normal" ModelLink="MarineDeath" SoundLink="Marine_DeathFXBloodSpray"/>
        <DeathArray index="Blast" ModelLink="MarineDeathBlast"/>
        <DeathArray index="Disintegrate" ModelLink="MarineDeathAcid" SoundLink="Uni_DeathFXAcid"/>
        <DeathArray index="Eat" AnimProps="Stand" ModelLink="MarineDeathEat" SoundLink="Marine_DeathFXEat"/>
        <DeathArray index="Eviscerate" ModelLink="MarineDeathEviscerate" SoundLink="Marine_DeathFXEviscerate"/>
        <DeathArray index="Fire" ModelLink="MarineDeathFire" SoundLink="Marine_DeathFXFire"/>
        <DeathArray index="Silentkill" SoundLink="Uni_DeathFXSilentKill"/>
        <DeathArray index="Squish" SoundLink="Marine_DeathFXBloodSpray"/>
        <DeathCustoms ModelLink="TerranSmallBiologicalUnitDeathLow" Name="AntiGravTerran"/>
        <DeathCustoms ModelLink="MarineDeathRagdoll" Name="RagdollBasic">
            <BodySquibs Name="None"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollSiegeTank" InheritsFrom="RagdollBasic"/>
        <DeathCustoms Name="RagdollNuke" InheritsFrom="RagdollBasic">
            <BodySquibs Name="Fire"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollFire" InheritsFrom="RagdollBasic">
            <BodySquibs Name="Fire"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollAcid" InheritsFrom="RagdollBasic">
            <BodySquibs Name="Acid"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollBaneling" InheritsFrom="RagdollBasic">
            <BodySquibs Name="Acid"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollUltraUnburrow" InheritsFrom="RagdollBasic"/>
        <DeathCustoms ActorModel="SpineCrawlerHurlDeathRagdollBloodPrevent" ModelLink="MarineDeathRagdoll" Name="RagdollSpineCrawlerHurl">
            <BodySquibs Name="None"/>
        </DeathCustoms>
        <DeathCustoms Name="RagdollOverkillDPRH" InheritsFrom="RagdollBasic"/>
        <DeathCustoms Name="RagdollOverkillDOI" InheritsFrom="RagdollBasic"/>
        <DeathCustoms Name="RagdollMidair" InheritsFrom="RagdollBasic"/>
        <DeathActorModel value="UnitDeathModelWithDeathMotionGround"/>
        <PortraitModel value="MarinePortrait"/>
        <StandAnimTurnTime value="4"/>
        <TerrainSquibs>
            <MovementDistance value="0.150000,0.150000"/>
            <IdlePeriod value="0.100000,0.250000"/>
            <RangeUp value="0.600000"/>
            <RangeDown value="0.600000"/>
            <AttachQuery Methods="AMFilterCenter"/>
            <Visuals TerrainPhysicsMaterial="Water" ActorModel="TerrainSquibHarnessContinuous" ModelLink="WaterRippleSmall"/>
        </TerrainSquibs>
        <WalkAnimMoveSpeed value="2.25"/>
        <AbilSoundArray AbilCmd="AP_Stimpack,Execute" Sound="Marine_StimPackVO"/>
        <BarOffset value="40"/>
        <BarWidth value="36"/>
        <GlossaryAnim value="Spell"/>
        <GroupIcon>
            <Image value="Assets\Textures\Wireframe-Terran-Marine.dds"/>
        </GroupIcon>
        <HeroIcon value="Assets\Textures\btn-unit-terran-marine.dds"/>
        <LifeArmorIcon value="Assets\Textures\btn-upgrade-terran-infantryarmorlevel0.dds"/>
        <UnitIcon value="Assets\Textures\btn-unit-terran-marine.dds"/>
        <Wireframe>
            <Image value="Assets\Textures\Wireframe-Terran-Marine.dds"/>
        </Wireframe>
    </CActorUnit>
    <CActorAction id="AP_MarineAttack" parent="GenericAttack" effectAttack="AP_GaussRifle">
        <LaunchAssets Model="MarineAttackLaunch" Sound="Marine_AttackLaunch"/>
        <ContainerAssets Model="MarineBunkerAttackLaunch" Sound="Marine_BunkerAttackLaunch"/>
        <ImpactMap index="None" Model="MarineAttackImpact" Sound="Marine_AttackImpact"/>
        <ImpactMap index="Flesh" ModelReaction="BloodTargetImpact"/>
        <DamageMap index="None" Sound=""/>
        <AcquisitionYawHalfArc value="10.000000"/>
        <AcquisitionPitchHalfArc value="15.000000"/>
        <AccuracyHalfArc value="5.000000"/>
    </CActorAction>
    <CActorModel id="AP_MarineCheerSquib" parent="ModelAdditionNoAnims">
        <On Terms="ActorCreation" Send="$Stand"/>
        <On Terms="AnimDone; AnimName Stand" Send="$Stand"/>
        <On Terms="AnimPlay; AnimName Stand" Send="Create MarineCheerSquibSound"/>
        <HostSiteOps Ops="SOpAttachWeapon"/>
        <Model value="MarineAttackLaunch"/>
    </CActorModel>
    <CActorSound id="AP_Marine_Land" parent="SoundOneShot">
        <Sound value="Marine_Land"/>
    </CActorSound>
    <CActorUnit id="AP_Marauder" parent="GenericUnitBase" unitName="AP_Marauder">
        <Aliases value="_UnitMedium"/>
        <Macros value="UnloadDropAnim"/>
        <On Terms="Abil.attack.ReadyStart" Send="AnimGroupApply Ready"/>
        <On Terms="ActorCreation" Send="StatusSet WeaponNext 0"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster" Send="StatusIncrement WeaponNext"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; IsStatus WeaponNext 2" Send="StatusSet WeaponNext 0"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; IsStatus WeaponNext 0" Send="AnimClear AttackRight"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; IsStatus WeaponNext 0" Send="AnimPlay AttackLeft Attack,Left"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; IsStatus WeaponNext 1" Send="AnimClear AttackLeft"/>
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; IsStatus WeaponNext 1" Send="AnimPlay AttackRight Attack,Right"/>
        <On Terms="UnitMovementUpdate.*.Walk" Send="AnimClear AttackLeft"/>
        <On Terms="UnitMovementUpdate.*.Walk" Send="AnimClear AttackRight"/>
        <On Terms="ModelEvent; ModelEventName AnimClear" Send="AnimClear AttackLeft"/>
        <On Terms="ModelEvent; ModelEventName AnimClear" Send="AnimClear AttackRight"/>
        <On Terms="Abil.attack.ReadyStop" Send="AnimGroupRemove Ready"/>
        <Model value="Marauder"/>
        <ModelFlags index="OutlineEmitter" value="1"/>
        <BuildModel value="Marauder"/>
        <DeathArray index="Normal" ModelLink="MarauderDeath" SoundLink="Marauder_Explode"/>
        <DeathArray index="Eviscerate" ModelLink="MarauderDeathEviscerate" SoundLink="Uni_DeathFXEviscerate"/>
        <DeathArray index="Silentkill" SoundLink="Uni_DeathFXSilentKill"/>
        <PlacementModel value="Marauder"/>
        <PortraitModel value="MarauderPortrait"/>
        <WalkAnimMoveSpeed value="2.25"/>
        <BarOffset value="60"/>
        <BarWidth value="50"/>
        <GroupIcon>
            <Image value="Assets\Textures\Wireframe-Terran-Marauder.dds"/>
        </GroupIcon>
        <HeroIcon value="Assets\Textures\btn-unit-terran-marauder.dds"/>
        <LifeArmorIcon value="Assets\Textures\btn-upgrade-terran-infantryarmorlevel0.dds"/>
        <SoundArray index="Ready" value="Marauder_Ready"/>
        <SoundArray index="Help" value="Marauder_Help"/>
        <SoundArray index="What" value="Marauder_What"/>
        <SoundArray index="Yes" value="Marauder_Yes"/>
        <SoundArray index="Attack" value="Marauder_Attack"/>
        <SoundArray index="Pissed" value="Marauder_Pissed"/>
        <UnitIcon value="Assets\Textures\btn-unit-terran-marauder.dds"/>
        <Wireframe>
            <Image value="Assets\Textures\Wireframe-Terran-Marauder.dds"/>
        </Wireframe>
    </CActorUnit>
    <CActorAction id="AP_MarauderAttack" parent="MarauderAttackBase" effectImpact="AP_PunisherGrenadesU" effectLaunch="AP_PunisherGrenadesLM">
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; !ValidateEffect AP_PunisherGrenadesResearched" Send="Create"/>
        <LaunchAttachQuery Methods="AMPatternMarauder"/>
        <LaunchAssets Model="MarauderAttackLaunch"/>
        <Missile value="AP_MarauderAttackMissile"/>
    </CActorAction>
    <CActorMissile id="AP_MarauderAttackMissile" parent="GenericAttackMissile" unitName="AP_PunisherGrenadesLMWeapon">
        <Model value="AP_PunisherGrenadesLMWeapon"/>
    </CActorMissile>
    <CActorAction id="AP_MarauderUpgradedAttack" parent="MarauderAttackBase" effectImpact="AP_PunisherGrenadesU" effectLaunch="AP_PunisherGrenadesLM">
        <On Terms="Effect.AP_PunisherGrenadesLM.Start; At Caster; ValidateEffect AP_PunisherGrenadesResearched" Send="Create"/>
        <LaunchAttachQuery Methods="AMPatternMarauder"/>
        <LaunchAssets Model="MarauderAttackLaunch"/>
        <Missile value="AP_MarauderAttackMissile"/>
        <ImpactMap index="None" Model="MarauderUpgradedAttackImpact"/>
    </CActorAction>
</Catalog>
